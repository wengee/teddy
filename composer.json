{
    "name": "fwkit/teddy",
    "description": "A PHP framework based on SLIM",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Fung Wing Kit",
            "email": "wengee@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Teddy\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "require": {
        "php": ">=8.1.0",
        "ext-swoole": ">=4.6.0",
        "slim/slim": "4.7.*",
        "slim/psr7": "1.3.*",
        "illuminate/support": "^8.57",
        "illuminate/config": "^8.57",
        "vlucas/phpdotenv": "^5.5",
        "monolog/monolog": "^2.1",
        "doctrine/annotations": "^1.10",
        "firebase/php-jwt": "^5.2",
        "league/flysystem": "^2.2",
        "league/event": "^2.2",
        "doctrine/dbal": "^3.0",
        "symfony/console": "^6.1"
    },
    "require-dev": {
        "guzzlehttp/guzzle": "^6.5",
        "aliyuncs/oss-sdk-php": "^2.4",
        "qcloud/cos-sdk-v5": "^2.5",
        "godruoyi/php-snowflake": "^1.0",
        "ramsey/uuid": "^3.9",
        "swoole/ide-helper": "^4.5"
    },
    "suggest": {
        "aliyuncs/oss-sdk-php": "Allows you to use Aliyun OSS",
        "qcloud/cos-sdk-v5": "Allows you to use TencentYun COS",
        "godruoyi/php-snowflake": "Allows you to use snowflake id generator",
        "ramsey/uuid": "Allows you to use db uuid column"
    }
}

{
    "name": "fwkit/teddy",
    "description": "A PHP framework based on SLIM",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Fung Wing Kit",
            "email": "wengee@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Teddy\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "require": {
        "php": ">=7.3.0",
        "ext-swoole": ">=4.4.0",
        "slim/slim": "4.5.*",
        "slim/psr7": "1.1.*",
        "illuminate/support": "^7.15",
        "illuminate/config": "^7.15",
        "vlucas/phpdotenv": "^4.0",
        "monolog/monolog": "^2.1",
        "doctrine/annotations": "^1.10",
        "firebase/php-jwt": "^5.2",
        "league/flysystem": "^1.0",
        "league/event": "^2.2",
        "fwkit/console": "^0.3.0"
    },
    "require-dev": {
        "guzzlehttp/guzzle": "6.*",
        "jacobcyl/ali-oss-storage": "^2.1",
        "overtrue/flysystem-cos": "^2.0",
        "godruoyi/php-snowflake": "^1.0",
        "ramsey/uuid": "^3.9",
        "fwkit/guzzle-hook": "^1.0",
        "swoole/ide-helper": "^4.5"
    },
    "suggest": {
        "jacobcyl/ali-oss-storage": "Allows you to use Aliyun OSS",
        "overtrue/flysystem-cos": "Allows you to use TencentYun COS",
        "godruoyi/php-snowflake": "Allows you to use snowflake id generator",
        "ramsey/uuid": "Allows you to use db uuid column"
    }
}
